        <TextBlock Grid.Column="1" Text="0" Foreground="Transparent" FontSize="30" x:Name="if_2_t"/>
        <TextBlock Foreground="RoyalBlue" HorizontalAlignment="Center"
                   VerticalAlignment="Center"
                   FontSize="30"
                  x:Name="tbTime"
                   Grid.Column="3" Grid.Row="8" Grid.ColumnSpan="2" Grid.RowSpan="2" FontFamily="{StaticResource pix}">
            <TextBlock.Effect>
                <DropShadowEffect
      ShadowDepth="4"
      Direction="330"
      Color="Black"
      Opacity="1"
     BlurRadius="4"/>
            </TextBlock.Effect>
        </TextBlock>
    </Grid>
