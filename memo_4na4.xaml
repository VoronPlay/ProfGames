        <Button Grid.Column="5" Content=""  x:Name="mode_1" Click="mode_1_Click" Style="{StaticResource Button_3}" FontFamily="{StaticResource pix}" Grid.Row="7" />
        <Button x:Name="m_2" Grid.Column="1" Grid.Row="7" BorderBrush="Transparent"/>
        <Button Grid.Column="1" Content=""  x:Name="mode_2" Click="mode_2_Click" Style="{StaticResource Button_3}" FontFamily="{StaticResource pix}" Grid.Row="7" />
        <Button x:Name="in_1" Grid.Column="1" Grid.Row="5" BorderBrush="Transparent"/>
        <Button Grid.Column="1" Content=""  x:Name="int_1" Click="int_1_Click" Style="{StaticResource Button_3}" FontFamily="{StaticResource pix}" Grid.Row="5" />
        <Button x:Name="in_2" Grid.Column="7"  Grid.Row="7" BorderBrush="Transparent"/>
        <Button Grid.Column="7" Content=""  x:Name="int_2" Click="int_2_Click" Style="{StaticResource Button_3}" FontFamily="{StaticResource pix}" Grid.Row="7" />
