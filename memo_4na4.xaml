        <Button x:Name="g_2" Grid.Column="3" BorderBrush="Transparent" Grid.Row="3" />
        <Button Grid.Column="3" Content=""  x:Name="Grid_2" Click="Grid_2_Click" Style="{StaticResource Button_3}" FontFamily="{StaticResource pix}" Grid.Row="3" />
        <Button Grid.Column="3" Grid.Row="5" BorderBrush="Transparent" x:Name="g_1"/>
        <Button Grid.Column="3" Content=""  x:Name="Grid_1" Click="Grid_1_Click" Style="{StaticResource Button_3}" FontFamily="{StaticResource pix}" Grid.Row="5" />
        <Button x:Name="v_11" Grid.Column="3" BorderBrush="Transparent" Grid.Row="7"></Button>
        <Button Grid.Column="3"   x:Name="void_1" Click="void_1_Click" Style="{StaticResource Button_3}" FontFamily="{StaticResource pix}" Grid.Row="7"></Button>
