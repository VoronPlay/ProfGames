        <Grid.RowDefinitions>
            <RowDefinition Height="30*"/>
            <RowDefinition Height="75*"/>
            <RowDefinition Height="30*"/>
            <RowDefinition Height="75*"/>
            <RowDefinition Height="30*"/>
            <RowDefinition Height="80*"/>
            <RowDefinition Height="30*"/>
            <RowDefinition Height="75*"/>
            <RowDefinition Height="50*"/>
        </Grid.RowDefinitions>
        <Image Source="Resource/fon_22.png" Grid.Column="0" Grid.Row="0" Grid.RowSpan="9" Grid.ColumnSpan="10" Height="2000" Width="2000" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        <Button x:Name="i_1" Grid.Column="1" BorderBrush="Transparent" Grid.Row="1" Grid.ColumnSpan="1" Grid.RowSpan="1"/>
        <Button Grid.Column="1" Grid.Row="1" Grid.ColumnSpan="1" Grid.RowSpan="1"  Style="{StaticResource Button_3}" FontFamily="{StaticResource pix}" x:Name="if_1" Click="if_1_Click"></Button>
        <Button x:Name="i_2" Grid.Column="5" BorderBrush="Transparent" Grid.Row="3"/>
        <Button Grid.Column="5"  x:Name="if_2" Click="if_2_Click" Style="{StaticResource Button_3}" FontFamily="{StaticResource pix}" Grid.Row="3"></Button>
        <Button x:Name="e_1" Grid.Column="5" BorderBrush="Transparent" Grid.Row="1"/>
        <Button Grid.Column="5" Content=""  x:Name="else_1" Click="else_1_Click" Style="{StaticResource Button_3}" FontFamily="{StaticResource pix}" Grid.Row="1" />
        <Button x:Name="e_2" Grid.Column="1" BorderBrush="Transparent" Grid.Row="3" />
        <Button Grid.Column="1" Content=""  x:Name="else_2" Click="else_2_Click" Style="{StaticResource Button_3}" FontFamily="{StaticResource pix}" Grid.Row="3" />
        <Button x:Name="x_1" Grid.Column="7" BorderBrush="Transparent" Grid.Row="3" />
        <Button Grid.Column="7" Content=""  x:Name="xaml_1" Click="xaml_1_Click" Style="{StaticResource Button_3}" FontFamily="{StaticResource pix}" Grid.Row="3" />
